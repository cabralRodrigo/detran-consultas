{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "filesystem",
      "library": "node_modules\\jquery\\dist\\",
      "destination": "wwwroot/lib/jquery/",
      "files": [ "jquery.js" ]
    },
    {
      "provider": "filesystem",
      "library": "node_modules/bootstrap/dist/js",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [ "bootstrap.js" ]
    },
    {
      "provider": "filesystem",
      "library": "node_modules/bootstrap/dist/css",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [ "bootstrap.css" ]
    },
    {
      "provider": "filesystem",
      "library": "node_modules/datatables.net/js/",
      "destination": "wwwroot/lib/datatables/",
      "files": [ "jquery.dataTables.js" ]
    },
    {
      "provider": "filesystem",
      "library": "node_modules/datatables.net-bs4/js/",
      "destination": "wwwroot/lib/datatables/",
      "files": [ "dataTables.bootstrap4.js" ]
    },
    {
      "provider": "filesystem",
      "library": "node_modules/datatables.net-bs4/css/",
      "destination": "wwwroot/lib/datatables/",
      "files": [ "dataTables.bootstrap4.css" ]
    },
    {
      "provider": "filesystem",
      "library": "node_modules/@fortawesome/fontawesome-free/css",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "all.css"
      ]
    },
    {
      "provider": "filesystem",
      "library": "node_modules/@fortawesome/fontawesome-free/webfonts/",
      "destination": "wwwroot/webfonts/"
